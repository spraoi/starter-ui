{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules coverage reports public .cache",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "deploy": "spraoi deploy-ui -c $config",
    "format": "yarn svgo && yarn prettier",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,gql,json,md,yml}\"",
    "serve": "gatsby serve --port 8000",
    "start": "gatsby develop",
    "svgo": "svgo --config node_modules/@spraoi/svgo-config/svgo.yml -f src/images/icons",
    "test": "jest --ci --runInBand",
    "test:e2e": "start-server-and-test serve http://localhost:8000 cypress:run"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/runtime": "^7.7.7",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.23.43",
    "@spraoi/babel-preset": "^0.23.43",
    "@spraoi/base": "^0.23.47",
    "@spraoi/gatsby-config": "^0.23.43",
    "@spraoi/helpers": "^0.23.43",
    "@spraoi/scripts": "^0.23.48",
    "@spraoi/validations": "^0.23.43",
    "@types/react": "^16.9.17",
    "@vx/responsive": "^0.0.192",
    "apollo-cache": "^1.3.4",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-utilities": "^1.3.3",
    "aws-amplify": "^2.2.1",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.26",
    "change-case": "^4.1.1",
    "final-form": "^4.18.6",
    "gatsby": "^2.18.16",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.3",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^5.2.1",
    "react-spinners": "^0.6.1",
    "styled-components": "^4.4.1",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.23.43",
    "@spraoi/jest-config": "^0.23.43",
    "@spraoi/prettier-config": "^0.23.43",
    "@spraoi/stylelint-config": "^0.23.43",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.4.0",
    "cypress": "^3.8.0",
    "cypress-file-upload": "^3.5.0",
    "eslint": "^6.7.2",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.1",
    "jest-junit": "^10.0.0",
    "jest-resolve": "^24.8.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.6",
    "stylelint": "^11.1.1",
    "typescript": "^3.7.3"
  }
}
