{
  "private": true,
  "name": "spraoi-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules public .cache && yarn",
    "format": "prettier --write \"*.{js,json,md}\" \"src/**/*.{js,json,css,scss,md\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --fix \"src/**/*.js\"",
    "lint:styles": "stylelint --fix \"src/**/*.js\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest"
  },
  "dependencies": {
    "@spraoi/validations": "^0.12.0",
    "gatsby": "^2.0.19",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@spraoi/babel-preset": "^0.12.0",
    "@spraoi/eslint-config": "^0.12.0",
    "@spraoi/gatsby-config": "^0.12.1",
    "@spraoi/jest-config": "^0.12.0",
    "@spraoi/prettier-config": "^0.12.0",
    "@spraoi/stylelint-config": "^0.12.1",
    "eslint": "^5.9.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0"
  }
}
