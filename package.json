{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules coverage reports public .cache",
    "deploy": "spraoi deploy -c $config",
    "format": "yarn svgo && yarn prettier",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,gql,json,md,yml}\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "svgo": "svgo --config node_modules/@spraoi/svgo-config/svgo.yml -f src/images/icons",
    "test": "jest --ci --runInBand"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.23.18",
    "@spraoi/babel-preset": "^0.23.0",
    "@spraoi/base": "^0.23.22",
    "@spraoi/gatsby-config": "^0.23.19",
    "@spraoi/helpers": "^0.23.18",
    "@spraoi/validations": "^0.23.18",
    "@types/react": "^16.9.9",
    "apollo-cache": "^1.3.2",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-utilities": "^1.3.2",
    "aws-amplify": "^1.2.2",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.20",
    "change-case": "^3.1.0",
    "final-form": "^4.18.5",
    "gatsby": "^2.17.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "react-final-form": "^6.3.0",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^5.2.1",
    "react-spinners": "^0.6.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.23.0",
    "@spraoi/jest-config": "^0.23.21",
    "@spraoi/prettier-config": "^0.23.19",
    "@spraoi/stylelint-config": "^0.23.18",
    "@spraoi/svgo-config": "^0.23.0",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jest-haste-map": "^24.9.0",
    "jest-junit": "^9.0.0",
    "jest-resolve": "^24.9.0",
    "prettier": "^1.18.2",
    "stylelint": "^11.1.1",
    "svgo": "^1.3.0",
    "typescript": "^3.6.4"
  }
}
