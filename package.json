{
  "private": true,
  "name": "spraoi-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules public .cache",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --fix --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --fix --ignore-path .gitignore \"src/**/*.js\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest"
  },
  "dependencies": {
    "@spraoi/validations": "^0.12.0",
    "gatsby": "^2.0.19",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/runtime": "^7.1.5",
    "@spraoi/babel-preset": "^0.12.3",
    "@spraoi/eslint-config": "^0.12.3",
    "@spraoi/gatsby-config": "^0.12.7",
    "@spraoi/jest-config": "^0.12.5",
    "@spraoi/prettier-config": "^0.12.3",
    "@spraoi/stylelint-config": "^0.12.3",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.9.0",
    "babel-preset-gatsby": "^0.1.4",
    "eslint": "^5.9.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0"
  }
}
