{
  "private": true,
  "name": "spraoi-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules public .cache",
    "deploy": "yarn build && spraoi deploy -c ./configs/$config.yml",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.19.0",
    "@spraoi/babel-preset": "^0.19.0",
    "@spraoi/base": "^0.19.12",
    "@spraoi/file-upload": "^0.19.9",
    "@spraoi/gatsby-config": "^0.19.13",
    "@spraoi/helpers": "^0.19.0",
    "@spraoi/validations": "^0.19.0",
    "apollo-client": "^2.5.1",
    "aws-amplify": "^1.1.22",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.9",
    "buffer": "^4.9.1",
    "final-form": "^4.12.0",
    "gatsby": "^2.2.6",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "polished": "^3.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.0",
    "react": "^16.8.4",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.8.4",
    "react-final-form": "^4.1.0",
    "react-helmet": "^5.2.0",
    "react-spinners": "^0.5.3",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.19.0",
    "@spraoi/jest-config": "^0.19.0",
    "@spraoi/prettier-config": "^0.19.0",
    "@spraoi/stylelint-config": "^0.19.0",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1"
  }
