{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules public .cache",
    "deploy": "yarn build && spraoi deploy -c ./configs/$config.yml",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.20.0",
    "@spraoi/babel-preset": "^0.20.0",
    "@spraoi/base": "^0.20.7",
    "@spraoi/gatsby-config": "^0.20.7",
    "@spraoi/helpers": "^0.20.0",
    "@spraoi/validations": "^0.20.7",
    "apollo-client": "^2.6.2",
    "aws-amplify": "^1.1.28",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-preset-gatsby": "^0.1.11",
    "change-case": "^3.1.0",
    "final-form": "^4.15.0",
    "gatsby": "^2.9.4",
