{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules public .cache",
    "deploy": "yarn build && spraoi deploy -c $config",
    "format": "yarn prettier && yarn svgo",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "svgo": "svgo --config node_modules/@spraoi/svgo-config/svgo.yml -f src/images/icons",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@aws-amplify/auth": "^1.2.31",
    "@aws-amplify/storage": "^1.1.0",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.21.0",
    "@spraoi/babel-preset": "^0.21.0",
    "@spraoi/base": "^0.21.4",
    "@spraoi/gatsby-config": "^0.21.2",
    "@spraoi/helpers": "^0.21.0",
    "@spraoi/validations": "^0.21.0",
    "apollo-client": "^2.6.3",
    "apollo-utilities": "^1.3.2",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.8",
    "change-case": "^3.1.0",
    "final-form": "^4.18.3",
    "gatsby": "^2.13.54",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "graphql": "^14.4.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-final-form": "^6.3.0",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^5.2.1",
    "react-spinners": "^0.5.13",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.21.0",
    "@spraoi/jest-config": "^0.21.0",
    "@spraoi/prettier-config": "^0.21.0",
    "@spraoi/stylelint-config": "^0.21.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.1",
    "jest-resolve": "^24.8.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "resolutions": {
    "@spraoi/base/aws-appsync/apollo-link-http/graphql": "^0.11.0"
  }
}
