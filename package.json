{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules coverage reports public .cache",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "deploy": "spraoi deploy-ui -c $config",
    "format": "yarn svgo && yarn prettier",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,gql,json,md,yml}\"",
    "serve": "gatsby serve --port 8000",
    "start": "gatsby develop",
    "svgo": "svgo --config node_modules/@spraoi/svgo-config/svgo.yml src/images/**/*.svg",
    "test": "jest --ci --runInBand",
    "test:e2e": "start-server-and-test serve http://localhost:8000 cypress:run"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@reach/router": "^1.3.4",
    "@spraoi/auth": "^0.29.5",
    "@spraoi/babel-preset": "^0.29.0",
    "@spraoi/base": "^0.29.8",
    "@spraoi/gatsby-config": "^0.29.0",
    "@spraoi/helpers": "^0.29.0",
    "@spraoi/scripts": "^0.29.8",
    "@spraoi/svgo-config": "^0.29.0",
    "@spraoi/validations": "^0.29.0",
    "@types/react": "^16.9.41",
    "@vx/responsive": "^0.0.197",
    "apollo-cache": "^1.3.5",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-utilities": "^1.3.4",
    "aws-amplify": "^3.0.18",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.5.1",
    "change-case": "^4.1.1",
    "final-form": "^4.20.1",
    "gatsby": "^2.23.17",
    "gatsby-cli": "2.12.54",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "polished": "^3.6.5",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.0",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "react-refresh": "^0.8.3",
    "react-spinners": "^0.9.0",
    "styled-components": "^5.1.1",
    "svgo": "^1.3.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.29.0",
    "@spraoi/jest-config": "^0.29.1",
    "@spraoi/prettier-config": "^0.29.0",
    "@spraoi/stylelint-config": "^0.29.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "cypress": "^4.9.0",
    "cypress-file-upload": "^4.0.7",
    "eslint": "^7.3.1",
    "jest": "^26.1.0",
    "jest-haste-map": "^26.1.0",
    "jest-junit": "^10.0.0",
    "jest-resolve": "^26.1.0",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0",
    "stylelint": "^13.6.1",
    "typescript": "^3.9.6"
  }
}
