    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.2.1",
    "react-final-form-listeners": "^1.0.2",
    "react-helmet": "^5.2.1",
    "react-spinners": "^0.5.4",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.20.7",
    "@spraoi/jest-config": "^0.20.7",
    "@spraoi/prettier-config": "^0.20.0",
    "@spraoi/stylelint-config": "^0.20.7",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.1",
    "jest-resolve": "^24.8.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
